                text="&lt;h4>&lt;span class=&quot;text-light&quot;>&lt;em>Adobe Acrobat User Group for Public Sector Professionals&lt;/em>&lt;br>&#xa;&lt;/span>&lt;/h4>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>&amp;nbsp;&lt;/span>&lt;/div>&#xa;"/>
            <text_cdc
                jcr:created="{Date}2017-08-18T20:54:17.897Z"

                jcr:lastModified="{Date}2017-08-21T15:18:17.348Z"
                jcr:lastModifiedBy="sale"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/text"
                text="&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>Learn how to deliver stand-out digital experiences by making manual, paper-based processes 100% digital. Securely and quickly integrate Adobe Sign into Adobe Acrobat. Join the Adobe team—we are here to help!&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp;&lt;/div>&#xa;"
                textIsRich="true"/>

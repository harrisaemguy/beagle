                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/text"
                text="&lt;h3>&lt;span class=&quot;text-light&quot;>Menu Names And Style Linking&lt;/span>&lt;/h3>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp;&lt;/div>&#xa;"
                textIsRich="true"/>
            <text_7fb
                jcr:created="{Date}2016-08-05T02:51:04.873-07:00"

                text="&lt;h3>&lt;span class=&quot;text-light&quot;>Release Notes&lt;/span>&lt;/h3>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp;&lt;/div>&#xa;"
                textIsRich="true"/>
            <text_9f9f
                jcr:created="{Date}2016-08-05T03:14:25.763-07:00"
                jcr:createdBy="ashahab"
                jcr:lastModified="{Date}2016-08-10T00:21:43.036-07:00"
                jcr:lastModifiedBy="ashahab"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/text"
                text="&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>For all fonts of family Source Code Pro: versions 2.010 Romans and 1.030 Italics created on Tue Jul 07 10:54:46 2015.&lt;br>&#xa; &lt;br>&#xa; version 2.010 (romans) &amp;amp; version 1.030 (italics) created 2015/07/07&lt;br>&#xa; &lt;br>&#xa; &amp;nbsp; &amp;nbsp; â Fixes hinting on Regular OE. Corrects path direction on invsmileface.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;Nudges the dot accent up to prevent from collapsing in CFF fonts. Adjusts design and positioning of cedillas.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;[Fixes hinting on Regular OE. Corrects path direction on invsmileface.]&lt;br>&#xa; &amp;nbsp;&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>version 2.005 (romans) &amp;amp; version 1.025 (italics) created 2015/06/25&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp;&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Adds characters that were missing for Arabic and Hebrew transliteration schemes.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Adds characters for coffee, heartblackheavy, robotface.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Updates design of ringhalfleftmod, ringhalfrightmod, glottalstopreversedmod, smileface, invsmileface, poopile.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Reorders some glyphs.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Fixes CVT errors in the italics.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Adds UI names to Character Variants features.&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&amp;nbsp; &amp;nbsp; â&amp;nbsp;&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>Adds SVG colored versions of these characters: â â âª â« â¥ â¦ âº â» â¤ â \ud83d\udca9 \ud83e\udd16 \ud83d\udd12&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>&amp;nbsp;&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>version 2.001 created 2015/06/09&amp;nbsp;&lt;/span>&lt;/div>&#xa;&lt;div class=&quot;text-6-block&quot;>&lt;span style=&quot;font-family: adobe-clean, sans-serif; font-size: 16px; font-style: normal; font-variant: normal; font-weight: 300; line-height: 21px;&quot;>&amp;nbsp;&lt;/span>&lt;/div>&#xa;"

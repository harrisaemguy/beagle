            </image_0_9643e80b-9c20-420f-aa7b-f86095d4d7bf>
            <text_9_8ecf72a3-057a-4d1e-ab4d-449376427b9d
                jcr:lastModified="{Date}2016-07-31T21:34:29.258-07:00"
                jcr:lastModifiedBy="datran"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/text"
                text="&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>OpenType lets font designers provide greater language support than any previous font format.&lt;/span>&lt;/div>&#xa;"/>
            <horizontalrule_df35
                jcr:created="{Date}2016-07-28T17:03:13.089-07:00"

            <text_79b75998-caff-482f-8cf6-fdf809054fbd
                jcr:lastModified="{Date}2017-04-26T23:19:58.903Z"
                jcr:lastModifiedBy="golee"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/text"
                text="&lt;div class=&quot;text-6-block&quot;>&lt;span class=&quot;text-light&quot;>The bitmap, outline, and metric data are combined into a single, cross-platform file in an OpenType font, simplifying font management.&lt;/span>&lt;/div>&#xa;"
                textIsRich="true"/>
            <horizontalrule_abb6
                jcr:created="{Date}2016-07-28T16:58:26.253-07:00"
                jcr:createdBy="datran"
                jcr:lastModified="{Date}2016-07-28T16:58:26.253-07:00"

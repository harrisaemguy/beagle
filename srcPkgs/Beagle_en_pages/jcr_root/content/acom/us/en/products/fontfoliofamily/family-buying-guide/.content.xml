                jcr:primaryType="nt:unstructured"
                sling:resourceType="beagle/components/table"
                tableDataHTML="&lt;table width=&quot;100%&quot; class=&quot;stripe-grey-white-reverse data-table-row-em&quot;>&#xa;&lt;thead>&lt;tr>&lt;th>&lt;div class=&quot;text-5-block&quot;>Whether you are a professional, student, or teacher, Adobe has a Font Folio offering for you.&lt;/div>&#xa;&lt;/th>&#xa;&lt;th>&lt;p>&lt;a href=&quot;/content/acom/us/en/products/fontfolio.html&quot; adhocenable=&quot;false&quot;>Font Folio 11.1&lt;/a>&lt;/p>&#xa;&lt;/th>&#xa;&lt;th>&lt;p>&lt;a href=&quot;/content/acom/us/en/products/fontfolio-education-essentials.html&quot; adhocenable=&quot;false&quot;>Font Folio Education Essentials 11&lt;/a>&lt;/p>&#xa;&lt;/th>&#xa;&lt;/tr>&lt;/thead>&lt;tbody>&lt;tr>&lt;td>Includes more than 2,400 high-quality OpenType® fonts from the Adobe Type Library&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes 500 OpenType fonts specifically chosen to provide depth, breadth, and value for higher education&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes fonts that support Central and Eastern European languages, as well as Greek and Cyrillic&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes fonts that support Japanese, Korean, Hebrew, Arabic, Thai, and Devanagari&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes the Adobe Type Library Reference Book, Fourth Edition&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes keyboard layouts for more than 40 Pi fonts&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Includes an OpenType User Guide (PDF)&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Recommended by AIGA, the professional association for design, for educators and students&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Requires proof of academic eligibility prior to shipment of order&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Limited to one purchase within a 12 month period&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Licensed for use on either 5, 10, or 20 computers&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>Licensed for use on one computer&lt;/td>&#xa;&lt;td>&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>●&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;/tbody>&lt;/table>"/>

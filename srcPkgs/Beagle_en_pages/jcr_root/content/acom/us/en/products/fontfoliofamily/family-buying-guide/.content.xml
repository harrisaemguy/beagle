                text="&lt;div class=&quot;text-5-block&quot;>Compare editions of Adobe® Font Folio® software.&lt;/div>&#xa;&lt;div class=&quot;text-5-block&quot;>&amp;nbsp;&lt;/div>&#xa;&lt;div class=&quot;text-5-block&quot;>Product editions&lt;/div>&#xa;&lt;div class=&quot;text-5-block&quot;>&amp;nbsp;&lt;/div>&#xa;"/>
            <table_90ff
                jcr:created="{Date}2016-11-23T10:52:38.419Z"

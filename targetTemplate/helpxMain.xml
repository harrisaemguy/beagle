<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="helpx/components/structure/helpxMain">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <globalnavheader
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dexter/components/restricted/globalNavHeader">
                <subnav jcr:primaryType="nt:unstructured">
                    <links
                        jcr:primaryType="nt:unstructured"
                        inheritCustomLinks="{Boolean}true"
                        inheritGeneratedLinks="{Boolean}true"/>
                </subnav>
            </globalnavheader>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
            <globalnavfooter
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dexter/components/restricted/globalNavFooter"/>
        </root>
    </jcr:content>
</jcr:root>
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/dexter/settings/wcm/templates/experience-fragment-web-variation"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="tt"
        sling:resourceType="dexter/components/structure/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <appbanner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dexter/components/restricted/appBanner">
                <closeCta
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/cta"
                    align="align-left"
                    linkTarget="_self"
                    linkText="close"
                    linkURL="#"
                    style="nostyle">
                    <icon
                        jcr:primaryType="nt:unstructured"
                        enabled="{Boolean}true"
                        graphic="#spectrum-icon-18-Close"
                        style="spectrum-Icon spectrum-Icon--sizeXS"/>
                </closeCta>
                <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/image"/>
                <download
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/cta"
                    align="align-left"
                    linkTarget="_self"
                    linkText="Buy"
                    linkURL="#"
                    style="nostyle">
                    <icon
                        jcr:primaryType="nt:unstructured"
                        enabled="{Boolean}false"
                        style="spectrum-Icon spectrum-Icon--sizeXS"/>
                </download>
                <publisher
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/text"
                    text="&lt;p>Adobe&lt;/p> "/>
                <visitAppStore
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/cta"
                    align="align-left"
                    linkTarget="_self"
                    linkText="iOS"
                    linkURL="#"
                    style="nostyle">
                    <icon
                        jcr:primaryType="nt:unstructured"
                        enabled="{Boolean}false"
                        style="spectrum-Icon spectrum-Icon--sizeXS"/>
                </visitAppStore>
                <visitPlayStore
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/cta"
                    align="align-left"
                    linkTarget="_self"
                    linkText="Google"
                    linkURL="#"
                    style="nostyle">
                    <icon
                        jcr:primaryType="nt:unstructured"
                        enabled="{Boolean}false"
                        style="spectrum-Icon spectrum-Icon--sizeXS"/>
                </visitPlayStore>
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/text"
                    text="&lt;p>Acrobat Reader&lt;/p> "/>
                <appStorePrice
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/text"
                    text="&lt;p>Buy in the App Store&lt;/p> "/>
                <playStorePrice
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dexter/components/content/text"
                    text="&lt;p>Buy in the Google Play Store&lt;/p> "/>
            </appbanner>
        </root>
    </jcr:content>
</jcr:root>